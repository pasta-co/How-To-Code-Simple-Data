;; Number -> Number
;; doubles n
(check-expect (doub 0) 0)
(check-expect (doub 4) 8)
(check-expect (doub 3.3) (* 2 3.3))
(check-expect (doub -1) -2)

;(define (doub n) 0) ; stub

(define (doub n)
  (* 2 n))
